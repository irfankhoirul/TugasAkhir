<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="proyekakhir.mapdemo" >

    <permission
        android:name="proyekakhir.mapdemo.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <permission
        android:name="proyekakhir.mapdemo.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="proyekakhir.mapdemo.permission.C2D_MESSAGE" />
    <uses-permission android:name="proyekakhir.mapdemo.permission.C2D_MESSAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <!-- Required OpenGL ES 2.0. for Maps V2 -->
    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <!-- Requires OpenGL ES version 2 -->
    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <!--
     To retrieve OAuth 2.0 tokens or invalidate tokens to disconnect a user. This disconnect
     option is required to comply with the Google+ Sign-In developer policies
    -->
    <uses-permission android:name="android.permission.USE_CREDENTIALS" /> <!-- To retrieve the account name (email) as part of sign-in: -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" /> <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".Activity.Activity0_SplashScreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.Activity1_Login"
            android:label="Login" >
        </activity>
        <activity
            android:name=".Activity.Activity5_Register"
            android:label="@string/title_activity___register" >
        </activity>
        <activity
            android:name=".Activity.KualitasJalan.Activity3_KualitasJalan"
            android:label="@string/title_activity___test_ride_app1"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Activity.Activity8_Setting"
            android:label="@string/title_activity___setting" >
        </activity>
        <activity
            android:name=".Activity.KualitasJalan.Activity3a_KualitasJalan_HasilPengukuran"
            android:label="@string/title_activity___test_ride_app1_result" >
        </activity>
        <activity
            android:name=".Activity.Activity2_MainMap"
            android:label="@string/title_activity___test_ride_app1_preparation" >
        </activity>
        <activity
            android:name=".DrawerActivity"
            android:label="@string/title_activity_drawer" >
        </activity>
        <activity
            android:name=".Activity.KualitasJalan.Activity6_ResultKualitasJalan"
            android:configChanges="keyboardHidden|orientation|screenSize"
            android:label="@string/title_activity_activity6__app1_result_map" >
        </activity>
        <activity
            android:name=".Activity.Activity9_UserDetails"
            android:label="@string/title_activity_activity9__user_details" >
        </activity>
        <activity
            android:name=".Activity.Activity10_About"
            android:label="@string/title_activity_activity10__about" >
        </activity>
        <activity
            android:name=".Activity.Activity9a_UserDetailsEdit"
            android:label="@string/title_activity_activity9a__user_details_edit" >
        </activity>
        <activity
            android:name=".Activity.KualitasJalan.Activity3b_KualitasJalan_HasilPengukuran_Preview"
            android:label="@string/title_activity_activity3b__app1_preview" >
        </activity>
        <activity
            android:name=".Activity.KualitasJalan.Activity6a_ResultKualitasJalan_ViewPoint"
            android:label="@string/title_activity_activity6a__app1_result_map_details" >
        </activity>
        <activity
            android:name=".Activity.Activity5a_RegisterEmailVerification"
            android:label="@string/title_activity_email_verification" >
        </activity>
        <activity
            android:name=".Activity.KualitasJalan.Activity6b_ResultKualitasJalan_FilterUmum"
            android:label="@string/title_activity_result_filter" >
        </activity>
        <activity
            android:name=".Activity.Activity11_Help"
            android:label="@string/title_activity_activity11__help" >
        </activity>
        <activity
            android:name=".Activity.KualitasJalan.Activity6c_ResultKualitasJalan_FilterKhusus"
            android:label="@string/title_activity_activity6c__app1_special_result" >
        </activity>
        <activity
            android:name=".Activity.TemukanLubang.Activity4_TemukanLubang"
            android:label="@string/title_activity_activity3c__discover_pothole" >
        </activity>
        <activity
            android:name=".Activity.TemukanLubang.Activity4a_TemukanLubang_HasilPengukuran"
            android:label="@string/title_activity_activity4a__pothole_result" >
        </activity>
        <activity
            android:name=".Activity.TemukanLubang.Activity4b_TemukanLubang_HasilPengukuran_Preview"
            android:label="@string/title_activity_activity4b__pothole_preview" >
        </activity>
        <activity
            android:name=".Activity.TemukanLubang.Activity7_ResultTemukanLubang"
            android:label="@string/title_activity_activity4c__show_pothole" >
        </activity>
        <activity
            android:name=".Activity.TemukanLubang.Activity7a_ResultTemukanLubang_ViewPoint"
            android:label="@string/title_activity_activity6d__pothole_details" >
        </activity>

        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <!-- for Gingerbread GSF backward compat -->
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <category android:name="proyekakhir.mapdemo" />
            </intent-filter>
        </receiver>

        <service
            android:name=".NonActivity.GcmService"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>

        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name=".Activity.TemukanLubang.Activity7b_ResultTemukanLubang_Filter"
            android:label="@string/title_activity_activity7b__result_temukan_lubang__filter" >
        </activity>
        <activity
            android:name=".Activity.Activity12_AccelerometerCheck"
            android:label="@string/title_activity_activity12__accelerometer_check" >
        </activity>
    </application>

</manifest>
