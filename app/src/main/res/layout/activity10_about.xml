<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ext="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="#eeeeee"
    tools:context="proyekakhir.mapdemo.Activity.Activity10_About">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"
            android:padding="5dp"
            android:layout_marginBottom="5dp">

            <ImageView
                android:layout_width="270dp"
                android:layout_height="94dp"
                android:id="@+id/imageView"
                android:src="@drawable/ic_surveyorider_icon"
                android:layout_marginBottom="25dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="25dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="Dikembangkan Oleh"
                android:id="@+id/textView38"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="10dp"
                android:textColor="#000000"
                android:layout_gravity="center_horizontal" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="IRFAN KHOIRUL MUHLISHIN"
                android:id="@+id/textView39"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="10dp"
                android:textColor="#000000"
                android:layout_gravity="center_horizontal" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"
            android:padding="5dp"
            android:layout_marginBottom="5dp" >

            <WebView
                android:background="#ffffff"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/webView"
                android:layout_marginLeft="5dp"
                android:layout_marginBottom="10dp"
                android:layout_marginRight="5dp" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"
            android:padding="5dp"
            android:layout_marginBottom="5dp"
            android:layout_gravity="center_horizontal">

            <ImageView
                android:layout_width="48dp"
                android:layout_height="32dp"
                android:id="@+id/imageView4"
                android:src="@drawable/ic_facebook"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1" />

            <ImageView
                android:layout_width="48dp"
                android:layout_height="32dp"
                android:id="@+id/imageView5"
                android:src="@drawable/ic_gooleplus"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1" />

            <ImageView
                android:layout_width="48dp"
                android:layout_height="32dp"
                android:id="@+id/imageView6"
                android:src="@drawable/ic_twitter"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1" />

            <ImageView
                android:layout_width="48dp"
                android:layout_height="32dp"
                android:id="@+id/imageView7"
                android:src="@drawable/ic_linkedin"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1" />

            <ImageView
                android:layout_width="48dp"
                android:layout_height="32dp"
                android:id="@+id/imageView8"
                android:src="@drawable/ic_github"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1" />

            <ImageView
                android:layout_width="48dp"
                android:layout_height="32dp"
                android:id="@+id/imageView10"
                android:src="@drawable/ic_gmail"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1" />

            <ImageView
                android:layout_width="48dp"
                android:layout_height="32dp"
                android:id="@+id/imageView9"
                android:src="@drawable/ic_stackoverflow"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="5dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="1" />

        </LinearLayout>

    </LinearLayout>

</ScrollView>
