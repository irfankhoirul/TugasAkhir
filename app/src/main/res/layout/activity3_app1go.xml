<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:weightSum="1">


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent">

        <ViewFlipper
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/viewFlipper2" >

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/_act6_panel_map"
                android:layout_weight="0.8">

                <fragment
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    xmlns:tools="http://schemas.android.com/tools"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"

                    android:id="@+id/map"
                    tools:context=".MapsActivity"
                    android:name="com.google.android.gms.maps.SupportMapFragment"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"

                    />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5pt"
                    android:layout_gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Latitude : "
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/textView2"
                        android:layout_gravity="right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Longitude : "
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/textView3"
                        android:layout_gravity="right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Speed : "
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/textView4"
                        android:layout_gravity="right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Distance : "
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/textView5"
                        android:layout_gravity="right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="X-Axis : "
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/textView6"
                        android:layout_gravity="right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Y-Axis : "
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/textView7"
                        android:layout_gravity="right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Z-Axis : "
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/textView8"
                        android:layout_gravity="right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Waktu : "
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/textView9"
                        android:layout_gravity="right" />
                </LinearLayout>

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="5pt"
                    android:layout_gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="0"
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/_act6_txt_detailLat"
                        android:layout_gravity="right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="0"
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/_act6_txt_detailLong"
                        android:layout_gravity="right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="0"
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/_act6_txt_detailSpeed"
                        android:layout_gravity="right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="0"
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/_act6_txt_detailDistance"
                        android:layout_gravity="right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="0"
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/_act6_txt_xaxis"
                        android:layout_gravity="right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="0"
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/_act6_txt_yaxis"
                        android:layout_gravity="right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="0"
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/_act6_txt_zaxis"
                        android:layout_gravity="right" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="0"
                        android:layout_marginBottom="5pt"
                        android:layout_marginLeft="10pt"
                        android:id="@+id/_act6_txt_time"
                        android:layout_gravity="right" />

                </LinearLayout>
            </LinearLayout>
        </ViewFlipper>
    </LinearLayout>


</LinearLayout>